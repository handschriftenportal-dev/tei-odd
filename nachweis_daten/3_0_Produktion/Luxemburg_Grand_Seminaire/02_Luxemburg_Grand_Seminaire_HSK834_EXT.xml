                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Universis sanctae Leodiensis ecclesiae filiis, et praecipue Mauritio ecclesiae Novimonasterii Hoyensis canonico, frater Aegidius"/>
