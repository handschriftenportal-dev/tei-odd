                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="31a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Eo tempore Servatius Tungrensi ecclesiae decimus praesidebat"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020444,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="1a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Gloria omnipotenti deo qui nostris ita nomen suum dignatus est glorificare temporibus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020445,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="90b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Hucusque gesta Tungrensium Traiectensiumque pontificum sunt digesta"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020446,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="95b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Huic successit semper memorandus venerabilis noster patronus Lambertus in ordine vicesimus nonus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020447,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="67a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Hunc igitur beatum Ebregiselum ad superna translatum beatus Iohannes vicesimus quintus Traiectensi ecclesiae succedit episcopus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020448,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="60a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Hunc sequitur electio pretiosissimi confessoris Christi et antistitis beati Gundulfi"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020449,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="57a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Igitur defuncto beato Domitiano beatus Monulfus in pontificali sede vicesimus primus successit episcopus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020450,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="63a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Igitur post discessum beati Gundulfi episcopi vicesimus tertius Traiectensi ecclesiae beatus mitissimus Christi confessor Perpetuus ordinatur episcopus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020451,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="307a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Igitur, ut diximus, antistite Otberto universae carnis viam ingresso convenientibus in unum fidelibus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020452,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="111b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Interea successit sancto Lamberto venerabilis pater Hubertus in ordine episcopali tricesimus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020453,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="75a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Omnipotens deus, qui dives est in misericordia, cuius natura bonitas"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020454,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="6b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Omnis antiquitas, ut ait oratorum maximus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020455,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="194b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Post decessum domini Balderici episcopi quadragesimus octavus Leodiensis episcopus monachus religiosus magnae virtutis vir"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020456,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="70a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Post haec sanctus Amandus vicesimus sextus subrogatur episcopus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020457,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="54a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Post hos vicesimus Traiectensi ecclesiae praesedit sanctissimus pater beatus Domitianus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020458,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="64b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Post sanctum Perpetuum Traiectensi ecclesiae substituitur vicesimus quartus episcopus sanctus Ebregiselus"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020459,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="267a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Qualiter vero brachium beati Iacobi apostoli, fratris beati Iohannis evangelistae, de Galecia"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020460,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="368b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Radulpho Leodiensi episcopo successor eligitur Albertus primus archidiaconus frater Henrici Lotharingiae ducis"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020461,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="493b"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Roberto igitur pontifice viam universae carnis ingresso"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020462,INI"/>
                <h1:Field Type="5230" Value="Registereintrag"/>
                <h1:Field Type="4665" Value="14a"/>
                <h1:Field Type="5650" Value="Initium">
                    <h1:Field Type="5666l" Value="Sub ipso enim tempore beatus Petrus ad metropolim Treverorum scilicet civitatem"/>
                    <h1:Field Type="5680" Value="lateinisch"/>
                </h1:Field>
            </h1:Block>
            <h1:Block Type="t2" CreationDate="19.09.2024 15:56:17" ChangeDate="19.09.2024 15:56:17" FieldsCount="71">
                <h1:Field Type="5001" Value="93020463,INI"/>
