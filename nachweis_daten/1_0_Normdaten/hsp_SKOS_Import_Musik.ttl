hspv-musicNotation:Orgeltabulatur a skos:Concept;
  skos:prefLabel "Orgeltabulatur"@de, "organ tablature"@en, "tablature d'orgue"@fr,
    "intavolatura d'organo"@it;
  skos:broader hspv-musicNotation:Tabulatur;
  skos:definition "Praktische Tabellierungen der Stimmen mit Notensystem und / oder Buchstaben oder Ziffern. Sie sind in den Ländern verschieden. Neben dt. Orgeltabulaturen gibt es auch spanische, italienische, englische, französische."@de;
  skos:narrower hspv-musicNotation:alte%20deutsche%20Orgeltabulatur, hspv-musicNotation:neue%20deutsche%20Orgeltabulatur,
    hspv-musicNotation:spanische%20Orgeltabulatur;
