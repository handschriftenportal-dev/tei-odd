hspv:BNDG-B726 a skos:Concept;
  skos:notation "BNDG-B726";
  skos:prefLabel "Heftung auf Bünde"@de, "Sewing on supports"@en;
  skos:altLabel "Bindung"@de;
  skos:broader hspv:BNDG-B859;
  skos:definition "Die Heftung auf Bünde ist seit etwa 600 n. Chr. bekannt und war das charakteristische Bindeverfahren des Abendlandes. Sie ist zwingend mit der handwerklichen Herstellung und dem Verfahren des angesetzten Bandes verbunden; daher wurde sie nach der Einführung des industriellen Deckenbandes immer seltener und kommt heute nur noch bei bibliophilen Büchern zum Einsatz. Abhängig von der Form der Bünde werden verschiedene Hefttechniken definiert."@de;
  skos:example hspv:37bb8ed8-794a-4782-863d-df9f82621514;
  skos:narrower hspv:BNDG-B118, hspv:BNDG-B260;
  skos:inScheme hspv:BNDG-B .

