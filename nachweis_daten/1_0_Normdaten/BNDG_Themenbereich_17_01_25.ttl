hspv:BNDG-K763 a skos:Concept;
  skos:notation "BNDG-K763";
  skos:prefLabel "Langriemenschließe"@de, "Long-strap fastening"@en;
  skos:broader hspv:BNDG-K311;
  skos:definition "Sitzt der Dorn bzw. das Dornlager am Buchdeckel, wird gewöhnlich ein langer Lederriemen verwendet, der über den gesamten Vorderschnitt des Buches und noch mindestens über die Buchdeckelkante reicht, ehe er in eine Ösenplatte mündet."@de;
  skos:example hspv:0ea93a78-502a-4d87-8ec5-4f858d7fc175;
  skos:inScheme hspv:BNDG-K .

