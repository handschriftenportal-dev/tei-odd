  skos:notation "BNDG-K257";
  skos:prefLabel "Bindeverschluss"@de, "tie fastening"@en;
  skos:altLabel "Beweglicher Verschluss"@de;
  skos:broader hspv:BNDG-K755;
  skos:definition "Bindeverschlüsse sind Buchverschlüsse, die lediglich aus beweglichen Teilen, konkret Kordeln, Bändern oder Riemen, bestehen."@de;
  dct:bibliographicCitation "Adler 2010, S. 5.";
