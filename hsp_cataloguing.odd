          <specGrpRef target="#bodyfixes"/>
          <specGrpRef target="#hdrfixes"/>
          <specGrpRef target="#msdescfixes"/>
          <classRef key="att.global.facs"/>
          <classRef key="att.global.linking"/>
          <classRef key="att.canonical"/>
          <dataSpec ident="teidata.temporal.iso" mode="delete"/>

          <!-- header Fixes -->
          <elementSpec ident="sourceDesc" mode="change" module="header">
            <desc>Descriptive text here </desc>
            <attList>
              <attDef ident="default" mode="delete"/>
            </attList>
          </elementSpec>

          <!-- body fixes-->
          <specGrp xml:id="bodyfixes">
            <elementSpec ident="title" mode="change" module="core">
              <attList>
                <attDef ident="level" mode="delete"/>
              </attList>
            </elementSpec>
          </specGrp>

          <!-- ############################################  Specifications body ##########################################################  -->

          <!-- element specification altIdentifier - Komponente Identifikation (altIdentifier)-->
          <elementSpec ident="altIdentifier" module="msdescription" mode="change">
            <attList>
              <attDef ident="type" mode="change" usage="req">
                <valList type="semi" mode="replace">
                  <valItem ident="corpus">
                    <desc xml:lang="en">corpus shelf number</desc>
                    <desc xml:lang="de">Identifikator</desc>
                  </valItem>
                  <valItem ident="former">
                    <desc xml:lang="en">former shelf number</desc>
                    <desc xml:lang="de">ehemalige Signatur</desc>
                  </valItem>
                  <valItem ident="hsp-ID">
                    <desc xml:lang="en">hsp-Kulturobjektdokument-ID</desc>
                    <desc xml:lang="de">hsp-Kulturobjektdokument-ID</desc>
                  </valItem>
                  <valItem ident="mxml-ID">
                    <desc xml:lang="en">Manuscripta-Medievalia-ID</desc>
                    <desc xml:lang="de">Manuscripta-Medievalia-ID</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
            <exemplum>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <msDesc status="extern" xml:id="HSP-4f5fe1f5-938c-329b-8f83-b7521f4806ac" xml:lang="de" subtype="medieval" type="hsp:description">
                  <msIdentifier>
                    <settlement key="soz_123456789">Karlsruhe</settlement>
                    <repository key="soz_30002072" ref="http://d-nb.info/gnd/2005707-6 https://sigel.staatsbibliothek-berlin.de/suche/?isil=DE-31">Badische Landesbibliothek</repository>
                    <idno>Cod. Karlsruhe 2912, II A 1, a</idno>
                    <altIdentifier type="hsp-ID">
                      <collection>Handschriftenportal Kulturobjektdokument-ID</collection>
                      <idno>HSP_123456-a7891234-a56789</idno>
                    </altIdentifier>
                    <altIdentifier type="mxml-ID">
                      <collection>Manuscripta-Medievalia-ID</collection>
                      <idno>obj_123456</idno>
                    </altIdentifier>
                    <altIdentifier type="corpus">
                      <collection>collection XYZ</collection>
                      <idno>123</idno>
                    </altIdentifier>
                    <altIdentifier type="former">
                      <settlement key="26cf9267-82fe-3bf1-a37-c9960658499f">Regensburg</settlement>
                      <repository key="654a4abc-3191-3e68-995b-4fdbd157cf9d">Sankt Emmeran</repository>
                      <idno>St. Emm 57</idno>
                    </altIdentifier>
                  </msIdentifier>
                </msDesc>
              </egXML>
            </exemplum>
          </elementSpec>

          <!-- element specification date -->
          <elementSpec ident="date" mode="change" module="core">
            <constraintSpec ident="dateSchematronConstraint" scheme="schematron">
              <desc>Constraint for <gi>date</gi> in <gi>surrogates</gi></desc>
              <constraint>
                <sch:pattern id="isoNormDate">
                  <sch:rule context="tei:surrogates/tei:bibl/tei:date">
                    <sch:assert test="@when[matches(.,'^[1,2]\d{3}-[0,1]\d{1}-\d{2}$')]"> @when requires a date in the standard form: yyyy-mm-dd. </sch:assert>
                  </sch:rule>
                </sch:pattern>
              </constraint>
            </constraintSpec>
            <!-- deleted due to incompatibility 
              <constraintSpec ident="date_check" scheme="schematron">
              <constraint>
                <sch:pattern>
                  <sch:rule context="tei:date">
                    <sch:assert test="@when or (@notAfter and @notBefore) or (@from and @to)">You must provide either @when or @to/@from, or @notAfter/@notBefore.</sch:assert>
                  </sch:rule>
                </sch:pattern>
              </constraint>
            </constraintSpec>
            -->
            <attList>
              <attDef ident="when" mode="change" usage="rec"/>
              <attDef ident="type" mode="change" usage="rec">
                <valList mode="change" type="semi">
                  <valItem ident="primary"/>
                  <valItem ident="secondary"/>
                </valList>
              </attDef>
            </attList>
          </elementSpec>


          <!--element specification decoDesc -Komponente Äußeres Kunst -->
          <elementSpec ident="decoDesc" module="msdescription" mode="change">
            <content>
              <rng:oneOrMore>
                <rng:ref name="decoNote"/>
              </rng:oneOrMore>
            </content>
            <exemplum>
              <p>decoDesc may be used when specifying a description of some illuminated aspects of a manuscript, otherwise it is to be described in msContents</p>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <phsyDesc>
                  <p>formal physical description</p>
                  <decoDesc>
                    <decoNote type="form"> insert the description of some illuminated aspects of a manuscript here </decoNote>
                  </decoDesc>
                </phsyDesc>
              </egXML>
            </exemplum>
          </elementSpec>

          <!-- element specification decoNote Komponente Äußeres Kunst und Inhalt Kunst -->
          <elementSpec ident="decoNote" module="msdescription" mode="change">
            <constraintSpec ident="contraintdecoNote" scheme="schematron">
              <constraint>
                <sch:pattern id="ÄußeresKunst">
                  <sch:rule context="tei:physDesc/tei:decoDesc/tei:decoNote">
                    <sch:assert test="@type eq 'form' "> It's possible to describe some illuminated aspects of a manuscript here </sch:assert>
                  </sch:rule>
                </sch:pattern>
              </constraint>
            </constraintSpec>
            <attList>
              <attDef ident="type" mode="replace" usage="req">
                <valList type="closed">
                  <valItem ident="form"/>
                  <valItem ident="content"/>
                </valList>
              </attDef>
            </attList>
            <exemplum>
              <p>decoDesc may be used when specifying a description of some illuminated aspects of a manuscript, otherwise it is to be described in msContents</p>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <phsyDesc>
                  <p>formal physical description</p>
                  <decoDesc>
                    <decoNote type="form"> insert the description of some illuminated aspects of a manuscript here </decoNote>
                  </decoDesc>
                </phsyDesc>
              </egXML>
            </exemplum>
          </elementSpec>

          <!-- element specification head - Komponente Kopf -->
          <!-- head content specifications moved to msDesc due to incompatibility with head in listbibl -->
          <elementSpec ident="head" module="core" mode="change">
            <constraintSpec ident="head" scheme="schematron">
              <constraint>
                <sch:pattern id="indexTermHead">
                  <!-- <sch:rule context="tei:term[@type eq 'title']">
                    <sch:assert test="normalize-space()" diagnostics="d_term_type_title_en d_term_type_title_de">Should contain a title</sch:assert>
                  </sch:rule>-->
                  <!-- material -->
                  <sch:rule context="tei:msDesc/tei:head/tei:index/tei:term[@type eq 'material_type']">
                    <sch:assert test=".= ('','paper','linen','palm','papyrus','parchment','other')" diagnostics="d_term_type_material_type_en d_term_type_material_type_de">
                      If type="material_type", text should contain 'paper', 'linen', 'palm', 'papyrus', 'parchment', 'other' or should stay empty. </sch:assert>
                  </sch:rule>
                  <!--measure-->
                  <!--<sch:rule context="tei:head/tei:index/tei:term[@type eq 'measure']">
                    <sch:assert test="normalize-space()" diagnostics="d_term_type_measure_en d_term_type_measure_de">Should contain the number of leaves e.g. 12 Bl.</sch:assert>
                  </sch:rule>-->
                  <sch:rule context="tei:head/tei:index/tei:term[@type eq 'measure_noOfLeaves']">
                    <sch:assert test=".[matches(.,'^[0-9]*?$')]" diagnostics="d_term_type_measure_noOfLeaves_en d_term_type_measure_noOfLeaves_de">The content may only contain numbers</sch:assert>
                  </sch:rule>
                  <!--dimensions-->
                  <!--<sch:rule context="tei:head/tei:index/tei:term[@type eq 'dimensions']">
                    <sch:assert test="normalize-space()" diagnostics="d_term_type_dimensions_en d_term_type_dimensions_de">Should contain the dimension of the manuscript e.g 30,5 × 22</sch:assert>
                  </sch:rule>-->
                  <sch:rule context="tei:head/tei:index/tei:term[@type='height']">
                    <sch:assert test=".[matches(.,'^([0-9]+(,5)?)?$')]" diagnostics="d_termMeasurement_en d_termMeasurement_de">Value of height may only be an integer or must be a float which ends with ,5 or is empty</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='width']">
                    <sch:assert test=".[matches(.,'^([0-9]+(,5)?)?$')]" diagnostics="d_termMeasurement_en d_termMeasurement_de">Value of width may only be an integer or must be a float which ends with ,5 or is empty</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='depth']">
                    <sch:assert test=".[matches(.,'^([0-9]+(,5)?)?$')]" diagnostics="d_termMeasurement_en d_termMeasurement_de">Value of depth may only be an integer or must be a float which ends with ,5 or is empty</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='dimensions_typeOfInformation']">
                    <sch:assert test=". =('','deduced','factual')" diagnostics="d_term_type_dimensions_typeOfInformation_en d_term_type_dimensions_typeOfInformation_de"> If type="dimensions_typeOfInformation" then content should be 'deduced' or 'factual' or should stay empty. </sch:assert>
                  </sch:rule>
                  <!-- format -->
                  <sch:rule context="tei:head/tei:index/tei:term[@type='format']">
                    <sch:assert test=".= ('','folio','quarto','octavo','smaller than octavo','larger than folio','other','oblong','square','long and narrow')" diagnostics="d_term_type_format_en d_term_type_format_de"> folio, quarto, octavo, other, smaller than octavo, larger than folio, oblong, long and narrow, square or empty </sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='format_typeOfInformation']">
                    <sch:assert test=".=('','deduced','factual', 'computed')" diagnostics="d_term_type_format_typeOfInformation_en d_term_type_format_typeOfInformation_de"> deduced, factual or empty. </sch:assert>
                  </sch:rule>
                  <!--origDate-->
                  <sch:rule context="tei:head/tei:index/tei:term[@type='origDate_type']">
                    <sch:assert test=". = ('','dated','datable')" diagnostics="d_term_type_origDate_type_en d_term_type_origDate_type_de"> If type="origDate_type", content should be dated or datable or empty </sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='origDate_notBefore']">
                    <sch:assert test=".[matches(.,'^((\-?[0-9]{4}\-(0[1-9]|1[012])\-(0[1-9]|[12][0-9]|3[01]))|(\-?[0-9]{4}\-(0[1-9]|1[012]))|(\-?[0-9]{4}))?$')]" diagnostics="d_origDate_en d_origDate_de"> The content should contain a date (year); several date-types are allowed: YYYY-MM-DD, -YYYY-MM-DD, YYYY-MM , -YYYY-MM or YYYY.</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='origDate_notAfter']">
                    <sch:assert test=".[matches(.,'^((\-?[0-9]{4}\-(0[1-9]|1[012])\-(0[1-9]|[12][0-9]|3[01]))|(\-?[0-9]{4}\-(0[1-9]|1[012]))|(\-?[0-9]{4}))?$')]" diagnostics="d_origDate_en d_origDate_de"> The content should contain a date (year); several date-types are allowed: YYYY-MM-DD, -YYYY-MM-DD, YYYY-MM , -YYYY-MM or YYYY. </sch:assert>
                  </sch:rule>
                  <!--form-->
                  <sch:rule context="tei:msDesc/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test=".= ('', 'codex', 'collection', 'composite', 'leporello', 'sammelband', 'fragment', 'printWithManuscriptParts', 'hostVolume', 'singleSheet', 'scroll', 'other')" diagnostics="d_term_type_form_en d_term_type_form_de"> If @type="form", Text should contain 'codex', 'collection', 'composite', 'leporello', 'sammelband', 'fragment', 'singleSheet', 'printWithManuscriptParts', 'hostVolume', 'scroll', 'other' or should stay empty</sch:assert>
                  </sch:rule>
                  <!-- status-->
                  <sch:rule context="tei:head/tei:index/tei:term[@type eq 'status']">
                    <sch:assert test=". = ('','missing','existent','destroyed','dismembered','unknown','displaced')" diagnostics="d_term_type_status_en d_term_type_status_de"> If @type="status", content should be: 'existent', 'missing', 'destroyed', 'unknown','displaced' or should stay empty </sch:assert>
                  </sch:rule>
                  <!--decoration and musicNotation-->
                  <sch:rule context="tei:head/tei:index/tei:term[@type='decoration']">
                    <sch:assert test=". =('','yes','no')" diagnostics="d_term_type_decoration_en d_term_type_decoration_de"> If type="decoration" then content should be 'yes' or 'no' or empty</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:head/tei:index/tei:term[@type='musicNotation']">
                    <sch:assert test=". =('','yes','no')" diagnostics="d_term_type_musicNotation_en d_term_type_musicNotation_de"> If type="musicNotation" then content should be 'yes' or 'no' or empty.</sch:assert>
                  </sch:rule>
                </sch:pattern>
                <sch:diagnostics>
                  <sch:diagnostic id="d_term_type_title_en" xml:lang="en">The index entry for the tite of the cultural object must not be empty! -- Please provide a title entry.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_title_de" xml:lang="de">Der Registereintrag zur Angabe des Titels des Kulturobjekts darf nicht leer sein! -- Bitte geben Sie einen Titel an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_material_type_en" xml:lang="en">The index entry to describe the material type in the element '<sch:value-of select="name()"/>' (of @type '<sch:value-of select="@type"/>') is given as '<sch:value-of select="."/>' which is invalid! -- Please use an entry from the designated controlled vocabulary.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_material_type_de" xml:lang="de">Der Registereintrag zur Beschreibung des Materials im Element '<sch:value-of select="name()"/>' (mit @type '<sch:value-of select="@type"/>') lautet ungültigerweise '<sch:value-of select="."/>'! -- Bitte geben Sie einen Eintrag aus dem dafür vorgesehenen kontrollierten Vokabular an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_measure_en" xml:lang="en">The entry to describe the measure of the manuscript given in the '<sch:value-of select="name()"/>' element must not be empty! -- Please enter information about the measure of the manuscript.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_measure_de" xml:lang="de">Die Angabe zum Umfang der Handschrift im Element '<sch:value-of select="name()"/>' darf nicht leer sein! -- Bitte machen Sie eine Angabe zum Umfang der Handschrift.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_measure_noOfLeaves_en" xml:lang="en">The value of the number of leaves '<sch:value-of select="."/>' given in the '<sch:value-of select="name()"/>' element must be an integer! -- Please use an integer value to describe the number of leaves.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_measure_noOfLeaves_de" xml:lang="de">Die Angabe der Blattzahl '<sch:value-of select="."/>' im Element '<sch:value-of select="name()"/>' muss eine Ganzzahl sein! -- Bitte geben Sie eine Ganzzahl an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_dimensions_en" xml:lang="en">The value of the dimensions of the cultural object must not be empty! -- Please enter the dimensions (e.g.  '30,5 × 22').</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_dimensions_de" xml:lang="de">Die Angabe zu den Abmessungen des Kulturobjekts darf nicht leer sein! -- Bitte geben Sie die Abmessungen (z.B. '30,5 × 22') an.</sch:diagnostic>
                  <sch:diagnostic id="d_termMeasurement_en" xml:lang="en">The value of the measurement '<sch:value-of select="."/>' given in the '<sch:value-of select="name()"/>' element must be an integer or end with ',5' as decimal place! -- Please use an integer value or use ',5' for the decimal place. (This is a requirement for filtering measurements.)</sch:diagnostic>
                  <sch:diagnostic id="d_termMeasurement_de" xml:lang="de">Die Angabe der Abmessung '<sch:value-of select="."/>' im Element '<sch:value-of select="name()"/>' muss eine Ganzzahl sein oder mit ',5' als Nachkommastelle enden! -- Bitte geben Sie eine Ganzzahl an bzw. geben Sie die Nachkommastelle auf ',5' vereinheitlicht an. (Diese Vereinheitlichung ist eine Anforderung für die Filterung nach Abmessungen.)</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_dimensions_typeOfInformation_en" xml:lang="en">The description '<sch:value-of select="."/>' about the kind of information about the dimenstions of the cultural object is not valid! -- Please enter 'deduced' or 'factual' or provide an empty entry.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_dimensions_typeOfInformation_de" xml:lang="de">Die Angabe '<sch:value-of select="name()"/>' zur Art der Information über die Abmessungen des Kulturobjekts ist ungültig! -- Bitte geben Sie 'deduced' oder 'factual' an bzw. lassen Sie den Eintrag leer.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_format_en" xml:lang="en">The index entry to describe the format of the cultural object is '<sch:value-of select="."/>' which is invalid! -- Please use an entry from the designated controlled vocabulary.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_format_de" xml:lang="de">Der Registereintrag für das Format des Kulturobjekts enthält mit '<sch:value-of select="."/>' kein gültiges Formatschlagwort! -- Bitte geben Sie einen Eintrag aus dem dafür vorgesehenen kontrollierten Vokabular an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_format_typeOfInformation_en" xml:lang="en">The description '<sch:value-of select="."/>' about the kind of information about the format of the cultural object is not valid! -- Please enter 'deduced', 'factual', 'computed' or provide an empty entry.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_format_typeOfInformation_de" xml:lang="de">Die Angabe '<sch:value-of select="name()"/>'  zur Art der Information über das Format des Kulturobjekts ist ungültig! -- Bitte geben Sie 'deduced', 'factual' oder 'computed' an bzw. lassen Sie den Eintrag leer.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_origDate_type_en" xml:lang="en">The date type entry '<sch:value-of select="."/>' is not valid! -- Please enter 'dated' or 'datable' or provide an empty entry.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_origDate_type_de" xml:lang="de">Der Eintrag zum Datumstyp '<sch:value-of select="name()"/>' ist ungültig! -- Bitte geben Sie 'dated' oder 'datable' an bzw. lassen Sie den Eintrag leer.</sch:diagnostic>
                  <sch:diagnostic id="d_origDate_en" xml:lang="en">The date '<sch:value-of select="."/>' given in the '<sch:value-of select="name()"/>' element must be formated as an ISO date! -- Please format the date entry according to one of the following patterns: YYYY-MM-DD, -YYYY-MM-DD, YYYY-MM , -YYYY-MM or YYYY.</sch:diagnostic>
                  <sch:diagnostic id="d_origDate_de" xml:lang="de">Das Datum '<sch:value-of select="."/>' im Element '<sch:value-of select="name()"/>' ist nicht richtig als ISO-Datum formatiert angegeben! -- Bitte formatieren Sie die Datumsangabe entsprechend einem der folgenden Muster: JJJJ-MM-TT, -JJJJ-MM-TT, JJJJ-MM , -JJJJ-MM or JJJJ.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_form_en" xml:lang="en">The index entry to describe the form of the cultural object is '<sch:value-of select="."/>' which is invalid! -- Please use an entry from the designated controlled vocabulary.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_form_de" xml:lang="de">Der Registereintrag für die Form des Kulturobjekts enthält mit '<sch:value-of select="."/>' kein gültiges Formschlagwort! -- Bitte geben Sie einen Eintrag aus dem dafür vorgesehenen kontrollierten Vokabular an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_status_en" xml:lang="en">The index entry to describe the form type in the element '<sch:value-of select="name()"/>' (of @type '<sch:value-of select="@type"/>') is given as '<sch:value-of select="."/>' which is invalid! -- Please use an entry from the designated controlled vocabulary.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_status_de" xml:lang="de">Der Registereintrag für den Formtyp im Element '<sch:value-of select="name()"/>' (mit @type '<sch:value-of select="@type"/>') lautet ungültigerweise '<sch:value-of select="."/>'! -- Bitte geben Sie einen Eintrag aus dem dafür vorgesehenen kontrollierten Vokabular an.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_decoration_en" xml:lang="en">The index entry for decoration is given as '<sch:value-of select="."/>' which is invalid! -- Please use 'yes or 'no' as entry or keep it empty.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_decoration_de" xml:lang="de">Der Registereintrag für Buchschmuck lautet ungültigerweise '<sch:value-of select="."/>'! -- Bitte geben Sie entweder 'yes' oder 'no' an oder lassen Sie den Eintrag leer.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_musicNotation_en" xml:lang="en">The index entry for music notation is given as '<sch:value-of select="."/>' which is invalid! -- Please use 'yes or 'no' as entry or keep it empty.</sch:diagnostic>
                  <sch:diagnostic id="d_term_type_musicNotation_de" xml:lang="de">Der Registereintrag für Musiknotation lautet ungültigerweise '<sch:value-of select="."/>'! -- Bitte geben Sie entweder 'yes' oder 'no' an oder lassen Sie den Eintrag leer.</sch:diagnostic>
                </sch:diagnostics>
              </constraint>
            </constraintSpec>
          </elementSpec>


          <!-- element specification history - Komponente Geschichte -->
          <elementSpec ident="history" module="msdescription" mode="change">
            <content>
              <elementRef key="p"/>
            </content>

            <constraintSpec ident="history" scheme="schematron">
              <constraint>
                <sch:pattern id="history">
                  <sch:rule context="tei:history">
                    <sch:report test="text()[normalize-space()]" diagnostics="d_history_text_en d_history_text_de">A history element must not contain text outside of its p element!</sch:report>
                  </sch:rule>                  
                  <sch:rule context="tei:history">
                    <sch:assert test="count(tei:p) eq 1" diagnostics="d_history_p_cardinality_en d_history_p_cardinality_de">A history element must have exactly one p element!</sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:history/tei:p">
                    <sch:assert test="empty(* except (tei:lb, tei:hi, tei:index, tei:quote, tei:ref, tei:persName, tei:orgName, tei:placeName))" diagnostics="d_history_p_content_en d_history_p_content_de">
                      The p element of a history element must contain only lb, hi, index, quote, ref, orgName, placeName, or indexName elements!</sch:assert>
                  </sch:rule>
                </sch:pattern>
                <sch:diagnostics>
                  <sch:diagnostic id="d_history_text_en" xml:lang="en">The history section must not contain text outside its paragraph! -- Please move the content into the paragraph.</sch:diagnostic>
                  <sch:diagnostic id="d_history_text_de" xml:lang="de">Der Abschnitt zur Geschichte darf keinen Inhalt außerhalb seines Absatzes enthalten! -- Bitte fügen Sie den Inhalt in den Absatz ein.</sch:diagnostic>
                  <sch:diagnostic id="d_history_p_cardinality_en" xml:lang="en">The history section must have exactly one paragraph! -- Please enter exactly one paragraph.</sch:diagnostic>
                  <sch:diagnostic id="d_history_p_cardinality_de" xml:lang="de">Der Abschnitt zur Geschichte muss genau einen Absatz enthalten! -- Bitte fügen Sie genau einen Absatz ein.</sch:diagnostic>
                  <sch:diagnostic id="d_history_p_content_en" xml:lang="en">The history section contains semantic markup which is not allowed! -- Please use allowed semantic markup only.</sch:diagnostic>
                  <sch:diagnostic id="d_history_p_content_de" xml:lang="de">Der Abschnitt zur Geschichte enthält unerlaubte semantische Auszeichnung! -- Bitte verwenden Sie nur die erlaubten semantischen Auszeichnungen.</sch:diagnostic>
                </sch:diagnostics>
              </constraint>
            </constraintSpec>

          </elementSpec>

          <!-- element specification idno -->
          <elementSpec ident="idno" mode="change" module="header">
            <attList>
              <attDef ident="sortKey" mode="change" usage="opt"/>
            </attList>
          </elementSpec>

          <!-- element specification msContents Komponente Inhalt (Text, Musik, Kunst, Äußeres Kunst)  -->
          <elementSpec ident="msContents" mode="change" module="msdescription">
            <content>
              <elementRef key="textLang" minOccurs="0"/>
              <elementRef key="msItem" minOccurs="1" maxOccurs="unbounded"/>
            </content>
            <exemplum>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <msContents>
                  <textLang mainLang="de" otherLangs="la fr">Handschrift enthält Texte in Deutsch, Latein und Französisch</textLang>
                  <msItem>
                    <note type="text">Inhalt Text Abschnitt</note>
                    <decoNote type="content">Inhalt Kunst</decoNote>
                    <decoNote type="form">Äußeres Kunst</decoNote>
                    <msItem>
                      <note type="text"> Inhalt Text untergeordneter Abschnitt</note>
                    </msItem>
                  </msItem>
                </msContents>
              </egXML>
            </exemplum>
          </elementSpec>


          <!-- element specification msDesc -->
          <elementSpec ident="msDesc" mode="change" module="msdescription">
            <content>
              <rng:ref name="msIdentifier"/>
              <!-- ODD 0.7.0 sequence ist recommended, due to compatibility to 0.6.1 every sequence is allowed so far by interleaving  -->
              <rng:element name="head">
                <rng:interleave>
                  <!-- title norm-->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_title</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>title</rng:value>
                      </rng:attribute>
                      <rng:optional>
                        <rng:text/>
                      </rng:optional>
                    </rng:element>
                  </rng:element>
                  <!-- title -->
                  <rng:element name="title">
                    <rng:zeroOrMore>
                      <rng:choice>
                        <rng:element name="hi">
                          <rng:attribute name="rend">
                            <rng:choice>
                              <rng:value>italic</rng:value>
                              <rng:value>italic sup</rng:value>
                              <rng:value>italic line-through</rng:value>
                              <rng:value>small-caps</rng:value>
                              <rng:value>sup small-caps</rng:value>
                              <rng:value>sup</rng:value>
                              <rng:value>sub</rng:value>
                            </rng:choice>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                        <rng:element name="lb"/>
                        <rng:text/>
                      </rng:choice>
                    </rng:zeroOrMore>
                  </rng:element>
                  <!-- note -->
                  <rng:element name="note">
                    <rng:attribute name="type">
                      <rng:value>headline</rng:value>
                    </rng:attribute>
                    <rng:zeroOrMore>
                      <rng:choice>
                        <rng:element name="hi">
                          <rng:attribute name="rend">
                            <rng:choice>
                              <rng:value>italic</rng:value>
                              <rng:value>italic sup</rng:value>
                              <rng:value>italic line-through</rng:value>
                              <rng:value>small-caps</rng:value>
                              <rng:value>sup small-caps</rng:value>
                              <rng:value>sup</rng:value>
                              <rng:value>sub</rng:value>
                            </rng:choice>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                        <rng:element name="lb"/>
                        <rng:text/>
                      </rng:choice>
                    </rng:zeroOrMore>
                  </rng:element>
                </rng:interleave>


                <rng:oneOrMore>
                  <!-- material -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_material</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>material</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                    <!-- wdh -->
                    <rng:oneOrMore>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>material_type</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                    </rng:oneOrMore>
                  </rng:element>

                  <!-- measure -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_measure</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>measure</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>measure_noOfLeaves</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                  </rng:element>

                  <!-- dimensions wdh -->
                  <rng:oneOrMore>
                    <rng:element name="index">
                      <rng:attribute name="indexName">
                        <rng:value>norm_dimensions</rng:value>
                      </rng:attribute>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>dimensions</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>height</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>width</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                      <rng:zeroOrMore>
                        <rng:element name="term">
                          <rng:attribute name="type">
                            <rng:value>depth</rng:value>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                      </rng:zeroOrMore>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>dimensions_typeOfInformation</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                    </rng:element>
                  </rng:oneOrMore>

                  <!-- format wdh -->
                  <rng:oneOrMore>
                    <rng:element name="index">
                      <rng:attribute name="indexName">
                        <rng:value>norm_format</rng:value>
                      </rng:attribute>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>format</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>format_typeOfInformation</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                    </rng:element>
                  </rng:oneOrMore>

                  <!-- origPlace wdh -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_origPlace</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>origPlace</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                    <!-- wdh. -->
                    <rng:oneOrMore>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>origPlace_norm</rng:value>
                        </rng:attribute>
                        <rng:optional>
                          <rng:attribute name="ref"/>
                        </rng:optional>
                        <rng:optional>
                          <rng:attribute name="key"/>
                        </rng:optional>
                        <rng:text/>
                      </rng:element>
                    </rng:oneOrMore>
                  </rng:element>

                  <!-- origDate -->
                  <rng:oneOrMore>
                    <rng:element name="index">
                      <rng:attribute name="indexName">
                        <rng:value>norm_origDate</rng:value>
                      </rng:attribute>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>origDate</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                      <!-- als Block -->
                      <rng:group>
                        <rng:element name="term">
                          <rng:attribute name="type">
                            <rng:value>origDate_notBefore</rng:value>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                        <rng:element name="term">
                          <rng:attribute name="type">
                            <rng:value>origDate_notAfter</rng:value>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                        <rng:element name="term">
                          <rng:attribute name="type">
                            <rng:value>origDate_type</rng:value>
                          </rng:attribute>
                          <rng:text/>
                        </rng:element>
                      </rng:group>
                    </rng:element>
                  </rng:oneOrMore>

                  <!-- textLang -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_textLang</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>textLang</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                    <!-- wdh. -->
                    <rng:oneOrMore>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>textLang-ID</rng:value>
                        </rng:attribute>
                        <rng:text/>
                      </rng:element>
                    </rng:oneOrMore>
                  </rng:element>

                  <!-- form -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_form</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>form</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                  </rng:element>

                  <!-- status -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_status</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>status</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                  </rng:element>

                  <!-- decoration -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_decoration</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>decoration</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                  </rng:element>

                  <!-- musicNotation -->
                  <rng:element name="index">
                    <rng:attribute name="indexName">
                      <rng:value>norm_musicNotation</rng:value>
                    </rng:attribute>
                    <rng:element name="term">
                      <rng:attribute name="type">
                        <rng:value>musicNotation</rng:value>
                      </rng:attribute>
                      <rng:text/>
                    </rng:element>
                  </rng:element>
                </rng:oneOrMore>
              </rng:element>
              <rng:zeroOrMore>
                <rng:choice>
                  <rng:ref name="msContents"/>
                  <rng:ref name="physDesc"/>
                  <rng:ref name="history"/>
                  <rng:ref name="additional"/>
                  <rng:ref name="msPart"/>
                </rng:choice>
              </rng:zeroOrMore>
            </content>



            <!-- constraints for msPart sequence -->
            <constraintSpec ident="msDesc" scheme="schematron">
              <constraint>
                <sch:pattern id="msPart_fragment">
                  <sch:rule context="tei:msPart[@type eq 'fragment']">
                    <sch:assert test="not(following-sibling::tei:msPart[@type eq 'binding'])"> Only msPart in the following order are allowed: binding, fragment, booklet, accMat, other </sch:assert>

                  </sch:rule>
                </sch:pattern>
                <sch:pattern id="msPart_booklet">
                  <sch:rule context="tei:msPart[@type eq 'booklet']">
                    <sch:assert test="not( following-sibling::tei:msPart[@type eq 'binding'] or following-sibling::tei:msPart[@type eq 'fragment'])"> Only msPart in the following order are allowed: binding, fragment, booklet, accMat, other </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <sch:pattern id="msPart_accMat">
                  <sch:rule context="tei:msPart[@type eq 'accMat']">
                    <sch:assert test="not( following-sibling::tei:msPart[@type eq 'binding'] or following-sibling::tei:msPart[@type eq 'fragment'] or following-sibling::tei:msPart[@type eq 'booklet'])"> Only msPart in the following order are allowed: binding, fragment, booklet, accMat, other </sch:assert>
                    
                  </sch:rule>
                </sch:pattern>
                <sch:pattern id="msPart_other">
                  <sch:rule context="tei:msPart[@type eq 'other']">
                    <sch:assert test="not( following-sibling::tei:msPart[@type eq 'binding'] or following-sibling::tei:msPart[@type eq 'fragment'] or following-sibling::tei:msPart[@type eq 'booklet'] or following-sibling::tei:msPart[@type eq 'accMat'])"> Only msPart in the following order are allowed: binding, fragment, booklet, accMat, other </sch:assert>
                  </sch:rule>
                </sch:pattern>
              </constraint>
            </constraintSpec>
            <attList>
              <attDef ident="xml:id" mode="change" usage="rec"/>
              <attDef ident="xml:lang" mode="change" usage="rec"/>
              <attDef ident="type" mode="replace" usage="rec">
                <valList type="closed" mode="replace">
                  <valItem ident="hsp:object">
                    <desc xml:lang="en">The TEI file represents (and contains) a <soCalled>Cultural Object Document</soCalled></desc>
                    <desc xml:lang="de">Die TEI-Datei repräsentiert und enthält ein <soCalled>Kultur Objekt Dokument</soCalled>.</desc>
                  </valItem>
                  <valItem ident="hsp:description">
                    <desc xml:lang="en">The TEI file represents (and contains) a manuscript description.</desc>
                    <desc xml:lang="de">Die TEI-Datei repräsentiert und enthält eine Handschriftenbeschreibung.</desc>
                  </valItem>
                  <valItem ident="hsp:description_retro"/>
                </valList>
              </attDef>
              <attDef ident="subtype" mode="replace" usage="rec">
                <valList type="closed" mode="replace">
                  <valItem ident="medieval"/>
                  <valItem ident="illuminated"/>
                </valList>
              </attDef>
              <attDef ident="status" mode="replace" usage="rec">
                <valList type="closed" mode="replace">
                  <valItem ident="intern"/>
                  <valItem ident="extern"/>
                </valList>
              </attDef>
            </attList>
            <exemplum>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <msDesc xml:id="__UUID__" xml:lang="de" status="intern" type="hsp:description" subtype="medieval">
                  <!-- manuscript description -->
                </msDesc>
              </egXML>
            </exemplum>
          </elementSpec>

          <!-- element specification msIdentifier -->


          <!-- element specification msItem Komponente Abschnitt -->
          <elementSpec ident="msItem" mode="change" module="msdescription">
            <content>
              <!-- 
                * msItem has zeroOrMore msItem-Siblings 
                * msItem has zeroOrMore children note, decoNote, msItem 
                * msItem has zero or one textlang and if it has textLang it is its first child 
              -->
              <elementRef key="textLang" minOccurs="0" maxOccurs="1"/>
              <alternate minOccurs="1" maxOccurs="unbounded">
                <elementRef key="note"/>
                <elementRef key="decoNote"/>
                <elementRef key="msItem"/>
                <elementRef key="index" minOccurs="0"/>
              </alternate>
            </content>
          </elementSpec>


          <!-- element specification msPart - Komponenten msPart -->
          <elementSpec ident="msPart" module="msdescription" mode="change">
            <content>
              <!-- Komponente Identifikation -->
              <rng:element name="msIdentifier">
                <rng:optional>
                  <rng:ref name="settlement"/>
                </rng:optional>
                <rng:optional>
                  <rng:ref name="repository"/>
                </rng:optional>
                <rng:ref name="idno"/>
                <rng:optional>
                  <rng:element name="altIdentifier">
                    <rng:attribute name="type">
                      <rng:value>former</rng:value>
                    </rng:attribute>
                    <!-- one of the following 3 elements -->
                    <rng:oneOrMore>
                      <rng:optional>
                        <rng:ref name="settlement"/>
                      </rng:optional>
                      <rng:optional>
                        <rng:ref name="repository"/>
                      </rng:optional>
                      <rng:ref name="idno"/>
                    </rng:oneOrMore>
                  </rng:element>
                </rng:optional>
              </rng:element>


              <!-- choice für Komponente Kopf -->
              <rng:choice>
                <rng:choice>
                  <!-- Komponente Sonstiges,contains only one or more p elements -->
                  <rng:oneOrMore>
                    <rng:ref name="p"/>
                  </rng:oneOrMore>
                </rng:choice>
                <!-- Komponente Kopf für accMat, fragment, booklet, binding-->
                <rng:choice>
                  <rng:group>
                    <rng:element name="head">
                      <!-- ODD 0.7.0 sequence ist recommended, due to compatibility to 0.6.1 every sequence is allowed so far by interleaving  -->
                      <rng:interleave>
                        <!-- title norm -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_title</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>title</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                        </rng:element>
                        <!-- title -->
                        <rng:element name="title">
                          <rng:optional>
                            <rng:text/>
                          </rng:optional>
                        </rng:element>
                        <!-- headline -->
                        <rng:element name="note">
                          <rng:attribute name="type">
                            <rng:value>headline</rng:value>
                          </rng:attribute>
                          <rng:optional>
                            <rng:text/>
                          </rng:optional>
                        </rng:element>
                      </rng:interleave>
                      <rng:oneOrMore>
                        <!-- material -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_material</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>material</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                          <!-- wdh -->
                          <rng:oneOrMore>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>material_type</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                          </rng:oneOrMore>
                        </rng:element>

                        <!-- measure -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_measure</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>measure</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>measure_noOfLeaves</rng:value>
                            </rng:attribute>
                            <rng:text/>
                          </rng:element>
                        </rng:element>

                        <!-- dimensions -->
                        <rng:oneOrMore>
                          <rng:element name="index">
                            <rng:attribute name="indexName">
                              <rng:value>norm_dimensions</rng:value>
                            </rng:attribute>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>dimensions</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>height</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>width</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>depth</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>dimensions_typeOfInformation</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                          </rng:element>
                        </rng:oneOrMore>

                        <!-- format -->
                        <rng:oneOrMore>
                          <rng:element name="index">
                            <rng:attribute name="indexName">
                              <rng:value>norm_format</rng:value>
                            </rng:attribute>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>format</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>format_typeOfInformation</rng:value>
                              </rng:attribute>
                              <rng:text/>
                            </rng:element>
                          </rng:element>
                        </rng:oneOrMore>

                        <!-- origPlace -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_origPlace</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>origPlace</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                          <!-- wdh. -->
                         <rng:oneOrMore>
                      <rng:element name="term">
                        <rng:attribute name="type">
                          <rng:value>origPlace_norm</rng:value>
                        </rng:attribute>
                        <rng:optional>
                          <rng:attribute name="ref"/>
                        </rng:optional>
                        <rng:optional>
                          <rng:attribute name="key"/>
                        </rng:optional>
                        <rng:text/>
                      </rng:element>
                    </rng:oneOrMore>
                        </rng:element>

                        <!-- origDate -->
                        <rng:oneOrMore>
                          <rng:element name="index">
                            <rng:attribute name="indexName">
                              <rng:value>norm_origDate</rng:value>
                            </rng:attribute>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>origDate</rng:value>
                              </rng:attribute>
                              <rng:optional>
                                <rng:text/>
                              </rng:optional>
                            </rng:element>
                            <!-- als Block -->
                            <rng:group>
                              <rng:element name="term">
                                <rng:attribute name="type">
                                  <rng:value>origDate_notBefore</rng:value>
                                </rng:attribute>
                                <rng:optional>
                                  <rng:text/>
                                </rng:optional>
                              </rng:element>
                              <rng:element name="term">
                                <rng:attribute name="type">
                                  <rng:value>origDate_notAfter</rng:value>
                                </rng:attribute>
                                <rng:optional>
                                  <rng:text/>
                                </rng:optional>
                              </rng:element>
                              <rng:element name="term">
                                <rng:attribute name="type">
                                  <rng:value>origDate_type</rng:value>
                                </rng:attribute>
                                <rng:text/>
                              </rng:element>
                            </rng:group>
                          </rng:element>
                        </rng:oneOrMore>

                        <!-- textLang -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_textLang</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>textLang</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                          <!-- wdh. -->
                          <rng:oneOrMore>
                            <rng:element name="term">
                              <rng:attribute name="type">
                                <rng:value>textLang-ID</rng:value>
                              </rng:attribute>
                              <rng:optional>
                                <rng:text/>
                              </rng:optional>
                            </rng:element>
                          </rng:oneOrMore>
                        </rng:element>

                        <!-- form -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_form</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>form</rng:value>
                            </rng:attribute>
                            <rng:text/>
                          </rng:element>
                        </rng:element>

                        <!-- status -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_status</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>status</rng:value>
                            </rng:attribute>
                            <rng:optional>
                              <rng:text/>
                            </rng:optional>
                          </rng:element>
                        </rng:element>

                        <!-- decoration -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_decoration</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>decoration</rng:value>
                            </rng:attribute>
                            <rng:text/>
                          </rng:element>
                        </rng:element>

                        <!-- musicNotation -->
                        <rng:element name="index">
                          <rng:attribute name="indexName">
                            <rng:value>norm_musicNotation</rng:value>
                          </rng:attribute>
                          <rng:element name="term">
                            <rng:attribute name="type">
                              <rng:value>musicNotation</rng:value>
                            </rng:attribute>
                            <rng:text/>
                          </rng:element>
                        </rng:element>
                      </rng:oneOrMore>
                    </rng:element>

                    <rng:zeroOrMore>
                      <rng:choice>
                        <rng:ref name="msContents"/>
                        <rng:ref name="physDesc"/>
                        <rng:ref name="history"/>
                        <rng:ref name="additional"/>
                        <rng:ref name="msPart"/>
                      </rng:choice>
                    </rng:zeroOrMore>
                  </rng:group>
                </rng:choice>
              </rng:choice>
            </content>

            <constraintSpec ident="msPart" scheme="schematron">
              <constraint>
                <!-- constraint for msPart = booklet-->
                <sch:pattern id="booklet">
                  <!-- @form -->
                  <sch:rule context="tei:msPart[@type eq 'booklet']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='booklet') or (.='')"> If msPart @type = 'booklet', then form should be 'booklet' or be empty </sch:assert>
                  </sch:rule>
                  <!-- @material_type -->
                  <sch:rule context="tei:msPart[@type eq 'booklet']/tei:head/tei:index/tei:term[@type eq 'material_type']">
                    <sch:assert test=".= ('','paper','linen','palm','papyrus','parchment','other')">
                      If type="material_type", text should contain 'paper', 'linen', 'palm', 'papyrus', 'parchment', 'other' or should stay empty </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint for msPart = binding -->
                <sch:pattern id="binding">
                  <!-- @material_type -->
                  <sch:rule context="tei:msPart[@type eq 'binding']/tei:head/tei:index/tei:term[@type eq 'material_type']">
                    <sch:assert test=".=('','wood','leather','parchment','textile materials','gold','silver','copper','brass','other')"> If msPart type="binding", material_type should contain 'wood','leather','parchment','textile materials','gold','silver','copper','brass', 'other' or should stay empty
                    </sch:assert>
                  </sch:rule>
                  <!-- @form -->
                  <sch:rule context="tei:msPart[@type eq 'binding']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='binding') or (.='')"> If msPart @type = 'binding', then form should be 'binding' or be empty </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint for msPart = accMat-->
                <sch:pattern id="accMat">
                  <!-- @material_type -->
                  <sch:rule context="tei:msPart[@type eq 'accMat']/tei:head/tei:index/tei:term[@type eq 'material_type']">
                    <sch:assert test=".= ('','paper','linen','palm','papyrus','parchment','other')">
                      If type="accMat", material_type should contain 'paper', 'linen', 'palm', 'papyrus', 'parchment', 'other' or should stay empty. </sch:assert>
                  </sch:rule>
                  <!-- @form -->
                  <sch:rule context="tei:msPart[@type eq 'accMat']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test=".=('','loose insert')"> If msPart @type = 'accMat', then form should be 'loose insert' or be empty </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint for msPart = fragment-->
                <sch:pattern id="fragment">
                  <!-- @material_type -->
                  <sch:rule context="tei:msPart[@type eq 'fragment']/tei:head/tei:index/tei:term[@type eq 'material_type']">
                    <sch:assert test=".= ('','paper','linen','palm','papyrus','parchment','other')">
                      If msPart type="fragment" material_type should contain 'paper', 'linen', 'palm', 'papyrus', 'parchment', 'other' or should stay empty. </sch:assert>
                  </sch:rule>
                  <!-- @form -->
                  <sch:rule context="tei:msPart[@type eq 'fragment']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='fragment') or (.='')" diagnostics="d_msPart_fragment_en d_msPart_fragment_de"> If msPart @type = 'fragment', then form should be 'fragment' or be empty </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <sch:diagnostics>
                  <sch:diagnostic id="d_msPart_fragment_en" xml:lang="en">The index entry (added for a manuscript fragment) to describe the form type in the element '<sch:value-of select="name()"/>' (of @type '<sch:value-of select="@type"/>') is given as '<sch:value-of select="."/>', but should be 'fragment' or empty! -- Please use 'fragment' as entry or keep it empty.</sch:diagnostic>
                  <sch:diagnostic id="d_msPart_fragment_de" xml:lang="de">Der für ein Handschriftenfragment eingefügte Registereintrag für den Formtyp im Element '<sch:value-of select="name()"/>' (mit @type '<sch:value-of select="@type"/>') lautet '<sch:value-of select="."/>', sollte aber 'fragment' oder leer sein! -- Bitte verwenden Sie den Eintrag 'fragment' oder lassen Sie den Eintrag leer.</sch:diagnostic>
                </sch:diagnostics>
                <!-- constraint if msPart binding, fragment, accMat has KOK fragment or accMat or other  -->
                <sch:pattern id="msPartInmsPartStandard">
                  <sch:rule context="tei:msPart[@type eq 'binding' or @type eq 'fragment' or @type eq 'accMat']/tei:msPart">
                    <sch:assert test="@type eq 'accMat' or @type eq 'fragment' or @type eq 'other'">
                      Only bindings, accMat and a fragment may contain further msParts with the following types: accMat, fragment or other </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint if msPart booklet has msPart fragment, booklet or accMat -->
                <sch:pattern id="msPartInmsPartBooklet">
                  <sch:rule context="tei:msPart[@type eq 'booklet']/tei:msPart">
                    <sch:assert test="@type eq 'fragment' or @type eq 'booklet' or @type eq 'accMat' or @type eq 'other'"> Only booklets may contain further msParts with the following types: accMat fragment, booklets and other</sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint for msPart in msPart @form -->
                <sch:pattern id="msPartInmsPartStandardForm">
                  <sch:rule context="tei:msPart/tei:msPart[@type eq 'fragment']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='fragment') or (.='')"> If msPart @type = 'fragment', then form should be 'fragment' or be empty </sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:msPart/tei:msPart[@type eq 'accMat']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='accMat') or (.='')"> If msPart @type = 'accMat', then form should be 'accMat' or be empty </sch:assert>
                  </sch:rule>
                  <sch:rule context="tei:msPart/tei:msPart[@type eq 'booklet']/tei:head/tei:index/tei:term[@type eq 'form']">
                    <sch:assert test="(.='booklet') or (.='')">If msPart @type = 'booklet' then form should be 'booklet' or be empty </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- constraint for 'other'/Sonstiges -->
                <sch:pattern id="idnoKomponenteSonstiges">
                  <sch:rule context="tei:msPart[@type eq 'other']/tei:msIdentifier/tei:idno">
                    <sch:assert test=".=('Sonstiges')"> If msPart @type = 'other' then idno contains 'Sonstiges' </sch:assert>
                  </sch:rule>
                </sch:pattern>

                <!-- constraint for msDesc altIdentifiers -->
                <sch:pattern id="msDesc_altIdentifier_corpus_idno">
                  <!-- idno for corpus -->
                  <sch:rule context="tei:msDesc/tei:msIdentifier/tei:altIdentifier[@type eq 'corpus']/tei:idno">
                    <sch:assert test="normalize-space()">If altIdentifier @type = 'corpus', then idno should not be empty</sch:assert>
                   </sch:rule>
                </sch:pattern>

                <sch:pattern id="msDesc_altIdentifier_former_idno">
                  <!-- idno for former -->
                  <sch:rule context="tei:msDesc/tei:msIdentifier/tei:altIdentifier[@type eq 'former']/tei:idno">
                    <sch:assert test="normalize-space()">If altIdentifier @type = 'former', then idno should not be empty</sch:assert>
                  </sch:rule>
                </sch:pattern>

                <!-- constraint for idno msPart alt -->
                <sch:pattern id="idnoMsPartAlt">
                  <sch:rule context="tei:msPart/tei:msIdentifier/tei:altIdentifier/tei:idno">
                    <sch:assert test="normalize-space()">If msPart then idno of altIdentifier should not be empty</sch:assert>
                  </sch:rule>
                </sch:pattern>

                <sch:pattern id="childKomponenteSonstiges">
                  <sch:rule context="tei:msPart[@type eq 'other']">
                    <sch:assert test="tei:p">msPart type="other" may only contain p Elements
                    </sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- only one msPart type="other" allowed-->
                <sch:pattern id="msPartoneP">
                  <sch:rule context="tei:msPart[@type eq 'booklet' or @type eq 'fragment' or @type eq 'binding' or @type eq 'accMat']">
                    <sch:assert test="count(tei:msPart[@type='other']) &lt; 2">only one msPart type='other' is allowed in one msPart</sch:assert>
                  </sch:rule>
                </sch:pattern>
                <!-- p not allowed in accMat, binding, fragment, booklet -->
                <sch:pattern id="msPartnoP">
                  <sch:rule context="tei:msPart[@type eq 'booklet' or @type eq 'fragment' or @type eq 'binding' or @type eq 'accMat']">
                    <sch:report test=". != tei:p">booklet, fragment, binding, accMat may not contain a p Element as direct child</sch:report>
                  </sch:rule>
                </sch:pattern>
              </constraint>
            </constraintSpec>

            <!-- attList for msPart -->
            <attList>
              <attDef ident="type" usage="req" mode="replace">
                <valList type="closed">
                  <valItem ident="booklet">
                    <desc>description for a booklet</desc>
                  </valItem>
                  <valItem ident="binding">
                    <desc>description of the binding</desc>
                  </valItem>
                  <valItem ident="fragment">
                    <desc>description for a fragment</desc>
                  </valItem>
                  <valItem ident="accMat">
                    <desc>description for accompanying material</desc>
                  </valItem>
                  <valItem ident="other">
                    <desc>other</desc>
                  </valItem>
                </valList>
              </attDef>
            </attList>
          </elementSpec>


          <!-- had to delete att.anchoring for <note>, because there was a problem within the <head> element and <note type="headline"> -->
          <elementSpec ident="note" module="core" mode="change">

            <constraintSpec ident="note" scheme="schematron">         
              <sch:pattern id="note">

                <!--
                <sch:rule context="tei:msDesc/tei:head/tei:note[@type eq 'headline']">
                  <sch:assert test="normalize-space()" diagnostics="d_note_headline_en d_note_headline_de">If note @type = 'headline', then note should not be empty</sch:assert>
                </sch:rule>
                -->
                <sch:rule context="tei:msDesc/tei:head/tei:note[@type eq 'headline']">
                  <!-- <sch:assert test="empty(* except(tei:hi))" -->
                  <sch:assert test="empty(* except (tei:hi, tei:lb))" diagnostics="d_note_headline_content_en d_note_headline_content_de">A note element of @type 'headline' should not contain any semantic markup elements!</sch:assert>
                </sch:rule>

                <sch:rule context="tei:note[@type eq 'text']">
                  <sch:assert test="normalize-space()" diagnostics="d_note_text_en d_note_text_de">If note @type = 'text', then note should not be empty</sch:assert>
                </sch:rule>

                <sch:rule context="tei:note[@type eq 'register']">
                  <sch:report test="text()[normalize-space()]" diagnostics="d_note_register_text_en d_note_register_text_de">A note element of @type 'register' must not contain text outside of its p element!</sch:report>
                </sch:rule>
                <sch:rule context="tei:note[@type eq 'register']">
                  <sch:assert test="count(tei:p) eq 1" diagnostics="d_note_register_p_cardinality_en d_note_register_p_cardinality_de">A note element of @type 'register' must have exactly one p element!</sch:assert>
                </sch:rule>
                <sch:rule context="tei:note[@type eq 'register']/tei:p">
                  <sch:assert test="empty(* except (tei:index))"  diagnostics="d_note_register_p_content_en d_note_register_p_content_de">The p element of a note element of @type 'register' must contain only index element(s)!</sch:assert>
                </sch:rule>
                <sch:rule context="tei:note[@type eq 'register']/tei:p">
                  <sch:assert test="count(tei:index) ge 1" diagnostics="d_note_register_p_index_cardinality_en d_note_register_p_index_cardinality_de">The p element of a note element of @type 'register' must have one or more index element(s)!</sch:assert>
                </sch:rule>
              </sch:pattern>

              <!--<sch:diagnostic id="d_note_headline_en" xml:lang="en">The note section for a headline should not be empty! -\- Please add a headline.</sch:diagnostic>-->
              <!--<sch:diagnostic id="d_note_headline_de" xml:lang="de">Die Anmerkung zur Überschrift darf nicht leer sein! -\- Bitte tragen Sie eine Überschrift ein.</sch:diagnostic>-->
              <sch:diagnostic id="d_note_headline_content_en" xml:lang="en">The note section for a headline should not contain semantic markup! -- Please remove semantic markup.</sch:diagnostic>
              <sch:diagnostic id="d_note_headline_content_de" xml:lang="de">Die Anmerkung zur Überschrift darf keine semantischen Auszeichnungen enthalten! -- Bitte entfernen Sie semantische Auszeichnungen.</sch:diagnostic>

              <sch:diagnostic id="d_note_text_en" xml:lang="en">The text note section should not be empty! -- Please add some text.</sch:diagnostic>
              <sch:diagnostic id="d_note_text_de" xml:lang="de">Die Textanmerkung darf nicht leer sein! -- Bitte fügen Sie Text ein.</sch:diagnostic>

              <sch:diagnostic id="d_note_register_text_en" xml:lang="en">The note register section must not contain text outside its paragraph! -- Please move the content into the paragraph.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_text_de" xml:lang="de">Der Anmerkungsbereich für Registereinträge darf keinen Inhalt außerhalb seines Absatzes enthalten! -- Bitte fügen Sie den Inhalt in den Absatz ein.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_cardinality_en" xml:lang="en">The note register section must have exactly one paragraph! -- Please enter exactly one paragraph.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_cardinality_de" xml:lang="de">Der Anmerkungsbereich für Registereinträge muss genau einen Absatz enthalten! -- Bitte fügen Sie genau einen Absatz ein.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_content_en" xml:lang="en">The note register section containts semantic markup which is not allowed! -- Please use allowed semantic markup only.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_content_de" xml:lang="de">Der Anmerkungsbereich für Registereinträge enthält unerlaubten Inhalt! -- Bitte verwenden Sie nur die erlaubten semantischen Auszeichnungen.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_index_cardinality_en" xml:lang="en">The note section for index entries does not contain an index entry! -- Please add at least one index entry.</sch:diagnostic>
              <sch:diagnostic id="d_note_register_p_index_cardinality_de" xml:lang="de">Der Anmerkungsbereich für Registereinträge enthält keinen Registereintrag! -- Bitte fügen Sie mindestens einen Registereintrag hinzu.</sch:diagnostic>
   
            </constraintSpec>

            <attList>
              <attDef ident="anchored" mode="delete"/>
              <attDef ident="targetEnd" mode="delete"/>
              <attDef ident="type" mode="change" usage="rec">
                <valList>
                  <valItem ident="text"/>
                  <valItem ident="music"/>
                </valList>
              </attDef>
            </attList>
            <exemplum>
              <egXML xmlns="http://www.tei-c.org/ns/Examples">
                <msContents>
                  <msItem>
                    <note type="text">description of the content of a manuscript</note>
                    <note type="music">description of the content of a musical notation</note>
                  </msItem>
                </msContents>
              </egXML>
            </exemplum>
          </elementSpec>

          <!-- element specification orgName Value List -->
          <elementSpec ident="orgName" mode="change" module="namesdates">
            <attList>
              <attDef ident="role" usage="rec" mode="replace">
                <datatype maxOccurs="unbounded">
                  <dataRef key="teidata.enumerated"/>
