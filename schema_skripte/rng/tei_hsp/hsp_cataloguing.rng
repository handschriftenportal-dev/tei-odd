         <sch:pattern xmlns="http://www.tei-c.org/ns/1.0"
                      xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns:xi="http://www.w3.org/2001/XInclude"
                      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                      id="history">
            <sch:rule context="tei:history">
               <sch:report test="text()[normalize-space()]"
                           diagnostics="d_history_text_en d_history_text_de">A history element must not contain text outside of its p element!</sch:report>
            </sch:rule>
            <sch:rule context="tei:history">
               <sch:assert test="count(tei:p) eq 1"
                           diagnostics="d_history_p_cardinality_en d_history_p_cardinality_de">A history element must have exactly one p element!</sch:assert>
            </sch:rule>
            <sch:rule context="tei:history/tei:p">
               <sch:assert test="empty(* except (tei:lb, tei:hi, tei:index, tei:quote, tei:ref, tei:persName, tei:orgName, tei:placeName))"
                           diagnostics="d_history_p_content_en d_history_p_content_de">
                      The p element of a history element must contain only lb, hi, index, quote, ref, orgName, placeName, or indexName elements!</sch:assert>
            </sch:rule>
         </sch:pattern>
         <sch:diagnostics xmlns="http://www.tei-c.org/ns/1.0"
                          xmlns:rng="http://relaxng.org/ns/structure/1.0"
                          xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                          xmlns:xi="http://www.w3.org/2001/XInclude"
                          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            <sch:diagnostic id="d_history_text_en" xml:lang="en">The history section must not contain text outside its paragraph! -- Please move the content into the paragraph.</sch:diagnostic>
            <sch:diagnostic id="d_history_text_de" xml:lang="de">Der Abschnitt zur Geschichte darf keinen Inhalt auÃerhalb seines Absatzes enthalten! -- Bitte fÃ¼gen Sie den Inhalt in den Absatz ein.</sch:diagnostic>
            <sch:diagnostic id="d_history_p_cardinality_en" xml:lang="en">The history section must have exactly one paragraph! -- Please enter exactly one paragraph.</sch:diagnostic>
            <sch:diagnostic id="d_history_p_cardinality_de" xml:lang="de">Der Abschnitt zur Geschichte muss genau einen Absatz enthalten! -- Bitte fÃ¼gen Sie genau einen Absatz ein.</sch:diagnostic>
            <sch:diagnostic id="d_history_p_content_en" xml:lang="en">The history section contains semantic markup which is not allowed! -- Please use allowed semantic markup only.</sch:diagnostic>
            <sch:diagnostic id="d_history_p_content_de" xml:lang="de">Der Abschnitt zur Geschichte enthÃ¤lt unerlaubte semantische Auszeichnung! -- Bitte verwenden Sie nur die erlaubten semantischen Auszeichnungen.</sch:diagnostic>
         </sch:diagnostics>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="additional">
      <element name="additional">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(additional) groups additional information, combining bibliographic information about a manuscript or other object, or surrogate copies of it, with curatorial or administrative information. [10.9. Additional Information]</a:documentation>
         <choice>
            <group>
               <optional>
                  <ref name="surrogates"/>
               </optional>
               <optional>
                  <ref name="listBibl"/>
               </optional>
            </group>
            <oneOrMore>
               <ref name="model.pLike"/>
            </oneOrMore>
         </choice>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="surrogates">
      <element name="surrogates">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(surrogates) contains information about any representations of the manuscript or other object being described which may exist in the holding institution or elsewhere. [10.9. Additional Information]</a:documentation>
         <zeroOrMore>
            <element name="bibl">
               <zeroOrMore>
                  <element name="ref">
                     <attribute name="type">
                        <choice>
                           <value>hsp:object</value>
                           <value>manifest</value>
                           <value>thumbnail</value>
                           <value>other</value>
                           <value>completeness</value>
                           <value>sourceType</value>
                           <value>digitalisationType</value>
                           <value>captureType</value>
                        </choice>
                     </attribute>
                     <attribute name="target"/>
                  </element>
               </zeroOrMore>
               <zeroOrMore>
                  <group>
                     <element name="orgName">
                        <attribute name="type">
                           <choice>
                              <value>hostInstitution</value>
                              <value>digitalisingInstitution</value>
                           </choice>
                        </attribute>
                        <attribute name="ref"/>
                        <text/>
                     </element>
                     <element name="placeName">
                        <attribute name="type">
                           <choice>
                              <value>hostInstitution</value>
                              <value>digitalisingInstitution</value>
                           </choice>
                        </attribute>
                        <attribute name="ref"/>
                        <text/>
                     </element>
                  </group>
               </zeroOrMore>
               <zeroOrMore>
                  <element name="date">
                     <optional>
                        <attribute name="when"/>
                        <attribute name="type">
                           <choice>
                              <value>published</value>
                              <value>digitalised</value>
                           </choice>
                        </attribute>
                     </optional>
                     <text/>
                  </element>
               </zeroOrMore>
            </element>
         </zeroOrMore>
         <ref name="att.global.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="msPart">
      <element name="msPart">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(manuscript part) contains information about an originally distinct manuscript or part of a manuscript, which is now part of a composite manuscript. [10.10. Manuscript Parts]</a:documentation>
         <element name="msIdentifier">
