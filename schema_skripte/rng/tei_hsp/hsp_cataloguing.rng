                        </attribute>
                        <element name="term">
                           <attribute name="type">
                              <value>origPlace</value>
                           </attribute>
                           <optional>
                              <text/>
                           </optional>
                        </element>
                        <oneOrMore>
                           <element name="term">
                              <attribute name="type">
                                 <value>origPlace_norm</value>
                              </attribute>
                              <optional>
                                 <attribute name="ref"/>
                              </optional>
                              <optional>
                                 <attribute name="key"/>
                              </optional>
                              <text/>
                           </element>
                        </oneOrMore>
                     </element>
                     <oneOrMore>
                        <element name="index">
                           <attribute name="indexName">
                              <value>norm_origDate</value>
                           </attribute>
                           <element name="term">
                              <attribute name="type">
                                 <value>origDate</value>
                              </attribute>
                              <optional>
                                 <text/>
                              </optional>
                           </element>
                           <group>
                              <element name="term">
                                 <attribute name="type">
                                    <value>origDate_notBefore</value>
                                 </attribute>
                                 <optional>
                                    <text/>
                                 </optional>
                              </element>
                              <element name="term">
                                 <attribute name="type">
                                    <value>origDate_notAfter</value>
                                 </attribute>
                                 <optional>
                                    <text/>
                                 </optional>
                              </element>
                              <element name="term">
                                 <attribute name="type">
                                    <value>origDate_type</value>
                                 </attribute>
                                 <text/>
                              </element>
                           </group>
                        </element>
                     </oneOrMore>
                     <element name="index">
                        <attribute name="indexName">
                           <value>norm_textLang</value>
                        </attribute>
                        <element name="term">
                           <attribute name="type">
                              <value>textLang</value>
                           </attribute>
                           <optional>
                              <text/>
                           </optional>
                        </element>
                        <oneOrMore>
                           <element name="term">
